}
.clinesfont{
  height:100%;
  width: 159px;
  font-size:12px;
  color:rgba(102,102,102,1);
  margin:0 26rpx 0 26rpx;
  position: absolute;
  top:50%;
  left: 50%;
  margin-left:-79.5px;
  margin-top:-18.5px;
  background: #f8f8f8;
  line-height: 37px;
  text-align: center;
}
.choiceimg{
  position: absolute;
  right:0;
  top:0;
  width:88rpx;
  height: 86rpx;
}
.nochoicebox{
  width:100%;
  height:42px;
  background:rgba(255,255,255,1);
  border-radius:1px;
  box-sizing: border-box;
  font-size:16px;
  color:rgba(244,67,54,1);
  position: fixed;
  bottom:0;
  left: 0;
  z-index: 10;
  line-height: 42px;
  text-align: center;
}