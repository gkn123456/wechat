/* pageA/pages/attestation/attestsuccess.wxss */
@import "../template/template.wxss";
page{
  width:100%;
  height: 100%;
}
.page{
  width:100%;
  height: 100%;
  background:linear-gradient(227deg,rgba(255,192,138,1) 0%,rgba(255,55,64,1) 100%);
  overflow: hidden;
}
.page1{
  display: flex;
  justify-content: center;
  margin-top:84px;
}
.page1 image{
  width:120px;
  height:119px;
}
.page2{
  display: flex;
  justify-content: center;
  margin-top:17px;
}
.page2 text{
  font-size:14px;
  color:rgba(255,255,255,1);
}
.page3{
  width:100%;
  padding:0 50px;
  box-sizing: border-box;
  margin-top:28px;
}
.page3s{
  width:100%;
  border-top:1px solid rgba(255,255,255,1);
}
.li{
  display: flex;
  margin-top:12px;
  width:100%;
}
.li1{
  font-size:14px;
color:rgba(255,255,255,1);
width:25%;
}
.li2{
  font-size:14px;
color:rgba(255,255,255,1);
margin-left: 30px;
width: 75%;
}
.page4{
  width:100%;
  position: fixed;
  bottom:50px;
  left:0;
  display: flex;
  justify-content: center;
}
.page4s{
  width:50px;
  height:50px;
  border-radius: 50%;
  background:rgba(255,255,255,1);
  display: flex;
  justify-content: center;
  align-items: center;
}
.page4 image{
  width:50px;
  height: 50px;
}