/* pageA/pages/attestation/attestlish.wxss */
@import "../template/template.wxss";
.headtips{
  width:100%;
  height: 31px;
  line-height: 31px;
  padding-left:16px;
  box-sizing: border-box;
  font-size:12px;
  color:rgba(170,170,170,1);
}
.alpage{
  width:100%;
  background: #fff;
  padding-left:16px;
  box-sizing: border-box;
}
.pageli{
  width: 100%;
  height:44px;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right:16px ;
  box-sizing: border-box;
  border-bottom:1px solid #ECECEC;
}
.pagelileft{
  font-size:14px;
  color:rgba(28,28,28,1);
}
.pageliright{
  display: flex;
  align-items: center;
}
.next{
  margin-left: 10px;
  width:37rpx;
height:28rpx;
}
.input{
  line-height: 44px;
  text-align: right;
  font-size:12px;
  color:rgba(28,28,28,1);
  width:100%;
}
.picker{
  font-size:12px;
  color:rgba(28,28,28,1);
}
.pagesli{
  width:100%;
  border-bottom:1px solid #ECECEC;
}
.pageslihead{
  display: flex;
  align-items: center;
  padding:14px 0 23px 0;
  box-sizing: border-box;
}
.pageslihead_text1{
  font-size:14px;
  color:rgba(28,28,28,1);
}
.pageslihead_text2{
  font-size:12px;
  color:rgba(170,170,170,1);
}
.pickbox{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding:0 30px 0 14px;
  box-sizing: border-box;
}
.pickimg{
  width: 140px;
  height: 105px;
  margin-bottom:20px;
}
.btn{
  background: #fff;
  width: 100%;
  padding: 14rpx 0;
  display: flex;
  justify-content: center;
  position: fixed;
  bottom:0;
  left: 0;
}
.btns{
  width:184rpx;
  height:68rpx;
  text-align: center;
  line-height: 68rpx;
  background:rgba(255,55,64,1);
  border-radius:8rpx;
  font-size:32rpx;
  font-weight:500;
  color:rgba(255,255,255,1);
  margin-left: 70px;
}
.btnss{
  width:184rpx;
  height:68rpx;
  text-align: center;
  line-height: 68rpx;
  font-size:16px;
  color:rgba(255,55,64,1);
  border-radius:8rpx;
  border:1px solid rgba(255,55,64,1);
  box-sizing: border-box;
}
.li{
  width: 100%;
  height:98rpx;
}