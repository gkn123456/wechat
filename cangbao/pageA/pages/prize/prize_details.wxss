@import "../template/template.wxss";
.p_head{
  width:100%;padding:16px 16px 6px 16px;box-sizing: border-box;position: relative;
}
.p_title{
  width:90%;font-size: 18px;color:#3f3e43;font-weight: bold;
}
#imgpbox{
  width:100%;height:100%;background: rgba(0, 0, 0, .9);position: fixed;top: 0;
  left: 0;z-index: 999;display: flex;align-items: center;justify-content: center;
}
#imgpbox image{width:100%;height:100%;}
.p_desc text{
  font-size: 14px;color:#165bfe;
}
.contlbox{
  width:100%;border-top:4px solid #fff;margin-top:15px;
}
#idsbox{
  width:100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
#l1 .contlbox:first-child{
  border:0;
}
.contli{
  width:90%;margin-bottom:10px;height:34px;background: #fff;box-shadow: 0 26rpx 20rpx 0 rgba(149,149,149,0.09);display: flex;align-items: center;      
  justify-content:space-between;border-radius: 24px;overflow: hidden;
}
.contli_quest{
  width:90%;margin-bottom:10px;height:34px;background: #fff;box-shadow: 0 26rpx 20rpx 0 rgba(149,149,149,0.09);display: flex;align-items: center;      
  justify-content:space-between;padding:0 20px;box-sizing: border-box;border-radius: 24px;
}
.contli_s1{
  width:100%;height:34px;background:rgba(0,178,106,.3);display: flex;align-items: center;      
  justify-content:space-between;padding:0 20px;box-sizing: border-box;
}
.contli_s2{
  width:100%;height:34px;background: #fff;display: flex;align-items: center;      
  justify-content:space-between;padding:0 20px;box-sizing: border-box;
}
.contlis{
  width:100%;margin-bottom:10px;height:34px;
}
.contlis_s{
  width:100%;height:100%;background: #fff;box-shadow: 0 26rpx 20rpx 0 rgba(149,149,149,0.09);display: flex;align-items: center;
  justify-content: space-between;padding:0 20px;box-sizing: border-box;
}
.contli_l{
  display: flex;align-items: center;
}
.contli_l view{
  font-size: 12px;
}
.contli_l icon{
  margin-left:10px;
}
.contli_r{
  display: flex;align-items: center;font-size: 12px;
}
.contli_r2{
  margin-left: 20px;
}
.btnbox{
  width:100%;display: flex;justify-content: center;margin-top:30px;
}
.success{
  width:100%;display: flex;justify-content: center;margin-top:20px;
}
.success text{
  font-size: 14px;color:red;
}
.btn{
  width:335px;height:44px;font-size:16px;line-height: 44px;text-align: center;background:rgba(255,55,64,1);border-radius:0;color:#fff;border:0;padding:0;
}
.endtime{
  font-size: 12px;color: #7f7f7f;padding-left: 16px;box-sizing: border-box;margin-bottom:5px;padding-right:16px;
}
.prizebox{
  width:100%;margin-top:20px;
}
.prize_title{
  font-size: 18px;color: #3f3e43;box-sizing: border-box;text-align: center;font-weight: bold;
}
.p_title{
  width:90%;font-weight: bold;font-size:14px;color:#08070c;
}
.p_title text{
  font-size: 12px;color:#7f7f7f;
}
.p_desc{
  width:100%;margin-top:10px;font-size: 13px;color:#08070c;padding:0 16px;box-sizing: border-box;
}
.p_desc text{
  font-size: 13px;color:#165bfe;
}
#share{
  background: #00b26a;width: 335px;height: 44px;text-align: center;line-height: 44px;color: #fff;font-size: 16px;border-radius: 0;border:0;padding:0;
}
.prizeb{
  width:100%;padding:0 10px;box-sizing: border-box;margin-top:10px;background: #fff;
}
.prizeb image{
  width:100%;height: 200px;
}
.prizeb view{
  font-size: 14px;color: #08070c;padding:0 10px;box-sizing: border-box;
}
.prizeb text{
  font-size: 14px;color: #08070c;
}
.btn1{
  background:#00b26a;width: 335px;height: 44px;text-align:center;line-height:44px;color:#fff;font-size:16px;
}
#imgprizebox{
  width:100%;justify-content: center;display: flex;align-items: center;flex-wrap: wrap;margin-top:10px;padding:0 20px;box-sizing: border-box;margin-bottom:10px;
}
#imgprizebox image{
  width:100%;height:100%;
}
.tips{
  width:100%;height:50px;background: red;display: flex;justify-content: center;align-items: center;color:#fff;font-size: 16px;margin-top:10px;
}
#vote_box{
  width:100%;overflow: hidden;margin:10px 0;
}