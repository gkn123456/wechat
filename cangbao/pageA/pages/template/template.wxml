<!-- 黑色 -->
<template name="template_o">
  <view class='nav bg-white' style='height:{{navH}}px'>
    <view class='nav-title'>
      <text>{{headtitle}}</text>
      <view class='nav-title_icon'>
        <view class='fh_box'>
          <image src='../img/template/f_h.png' class='f_hback'></image>
          <view style='width:100%;height:100%;position:absolute;top:0;left:0;display:flex;justify-content:space-between;'>
            <view style='width:50%;height:100%;' bindtap='back'><image src='../img/template/f_h1.png' mode='aspectFit' class='back'></image></view>
            <view style='width:50%;height:100%;' bindtap='backhome'><image src='../img/template/f_h2.png' mode='aspectFit' class='backhome'></image></view>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view style="height:{{navH}}px" class='nav_line_k'></view>
</template>
