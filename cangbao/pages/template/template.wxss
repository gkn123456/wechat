/* pages/template/template.wxss */
/* 自定义导航高度 */
.nav{
  width: 100%;
  height: 110rpx;
  overflow: hidden;
  z-index: 10;
  position: fixed;
  top:0;
  left:0;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
  letter-spacing:2px;
}
.o{
  color: #fff;
}
.nav-title_icon{
  position: absolute;
  height: 100%;
  top:0;
  left: 0;
  display: flex;
  align-items: center;
}
.nav .back{
  width: 9px;
  height: 14px;
  position: absolute;
  top:50%;
  left:19px;
  margin-top:-7px;
}
.backhome{
  width: 14px;
  height: 16px;
  position: absolute;
  top:50%;
  right:15px;
  margin-top:-8px;

}
.backhome1{
  width: 14px;
  height: 16px;
  
}
.f_hback{
  width: 87px;
  height: 32px;
}
.fh_box{
  width: 87px;
  height: 32px;
  margin-left: 32rpx;
  position: relative;
}
.fh_box1{
  width: 46px;
  height: 32px;
  margin-left: 32rpx;
  border: 1rpx solid rgba(0,0,0,0.08);
  border-radius: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}
.loading_box{
  width:100%;height:100%;background: #fff;position: fixed;top:0;left: 0;z-index: 8;
}
.loading_box_o{
  width:100%;height:40%;background:rgba(247,245,245,1);
}
.loading_box_o_top{
  width:100%;height:128rpx;background:linear-gradient(180deg,rgba(0,0,0,0.34) 0%,rgba(0,0,0,0) 100%);
}
.loading_box_t{
  width:100%;height:27px;background:rgba(241,241,241,1);display: flex;justify-content: center;align-items: center;
}
.loading_box_t_l{
  width: 20%;height:17px;background:rgba(228,228,228,1);border-radius:3px;margin-right:10px;
}
.loading_box_t_r{
  width:50%;height:17px;background:rgba(228,228,228,1);border-radius:3px;
}
.loading_box_h{
  width:100%;padding:19px 16px 20px 16px;box-sizing: border-box;
}
.loading_box_h_on{
  height:18px;background:#F7F7F7;border-radius:3px;margin-bottom:8px;
}
.loading_box_h_on1{
  height:16px;background:#F7F7F7;border-radius:3px;margin-bottom:8px;
}
.loading_box_h_on2{
  height:22px;background:#F7F7F7;border-radius:3px;
}