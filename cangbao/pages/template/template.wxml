<template name="payquota">
  <view id="xe" catchtouchmove='true' bindtap='closequota' data-id='3'>
    <view id="xe_box" catchtap='closequota' data-id='2'>
      <view class='xbox_top'><view class='xbox_topt1'>温馨提示</view><view class='xbox_topt2'>您本次支付金额较大，可能超出支付渠道单笔最高限额，如支付未能成功，请选其他支付方式</view>
      </view>
      <view class='xbox_bom'>
        <view class='xbox_bomb1' catchtap='closequota' data-id='1'>查看其他支付方式</view>
        <view class='xbox_bomb2' catchtap='closequota' data-id='0'>继续支付</view>
      </view>
      <view class='xbox_boms' catchtap='closequota' data-id='3'>取消</view>
    </view>
  </view>
</template>
<!-- 分享 -->
<template name="share">
  <view id='sharebox' bindtap='closeshare'>
    <view id="shareboxs" catchtap='openshare'>
      <view id="shareboxsclose"><image src="../img/addprice/chujia-btn-close.png" catchtap='closeshare'></image></view>
      <view id="shareboxcont">
        <view id="shareboxcont_li" catchtap='poster'>
          <image id="shar_liimg" src='../img/share_poster.png' />
          <text>分享海报</text>
        </view>
        <button id="shareboxcont_li" open-type="share" style='border:0;margin:0;padding:0;background-color:#fff;display:flex;' formType="submit" plain="true" hover-class="none" size="mini" catchtap='closeshare'>
          <image id="shar_liimg" src='../img/share_friend.png' />
          <text>好友</text>
        </button>
        <view id="shareboxcont_li" catchtap='copylink'>
          <image id="shar_liimg" src='../img/share_link.png' />
          <text>复制链接</text>
        </view>
        <view id="shareboxcont_li" catchtap='openreport'>
          <image id="shar_liimg" src='../img/share_jubao.png' />
          <text>举报</text>
        </view>
      </view>
    </view>
  </view>
</template>
