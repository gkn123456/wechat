<template name="template_loading">
  <view class="loading_box">
    <view class="loading_box_o">
      <view class="loading_box_o_top" style='height:{{navH}}px'></view>
    </view>
    <view class="loading_box_t"></view>
    <view class="loading_box_h">
      <view class="loading_box_h_on" style="width:90%;"></view>
      <view class="loading_box_h_on" style="width:50%;"></view>
      <view style="display: flex;align-items: center;">
        <view class="loading_box_h_on" style="width:10%;"></view><view class="loading_box_h_on" style="width:20%;margin-left:10px;"></view>
      </view>
      <view class="loading_box_h_on1" style="width:60%;"></view>
      <view class="loading_box_h_on1" style="width:45%;"></view>
      <view style="display: flex;align-items: center;justify-content: space-between;">
        <view class="loading_box_h_on2" style="width:22%;"></view><view class="loading_box_h_on2" style="width:22%;"></view><view class="loading_box_h_on2" style="width:22%;"></view><view class="loading_box_h_on2" style="width:22%;"></view>
      </view>
    </view>
    <view style="width:100%;height:10px;background:##FCFBFB;"></view>
    <view class="loading_box_h">
      <view class="loading_box_h_on" style="width:20%;margin-bottom:20px;"></view>
      <view class="loading_box_h_on" style="width:100%;"></view>
      <view class="loading_box_h_on" style="width:100%;"></view>
      <view class="loading_box_h_on" style="width:100%;"></view>
    </view>
  </view>
</template>
