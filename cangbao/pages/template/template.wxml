<template name="posterapp">
  <view id='sharebox' style='background:rgba(28,28,28,0.8);'>
    <image src="../../img/paysuccess/close.png" bindtap='closeposter' id="posterclose" />
    <view style='display:flex;justify-content: center;align-items: center;width；100%;height:100%;flex-direction: column;'>
      <view id="posterbox">
        <view id="share_pic">
          <image src="{{sharePicUrl}}" id="sharePic"></image>
        </view>
      </view>
      <view id='posterboxline'>
        <view id='clinest'></view><view id='clinesfont'>分享给朋友</view><view id='clinest'></view>
      </view>
      <view id="savebox">
        <view id="saveboxli" bindtap='saveimg'>
          <image src='../../img/share_btn_save@2x.png' /><text>保存到相册</text>
        </view>
      </view>
    </view>
    
    <!-- canvas S -->
    <canvas canvas-id="shareFrends" class="canvas"></canvas>
    <canvas canvas-id="canvascode" class="canvas_code"></canvas>
    <image id="coverimg" src='{{coverimg}}' bindload="bindload"></image>
  </view>
</template>
