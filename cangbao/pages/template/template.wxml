<template name="report">
  <view id='sharebox' bindtap='closereport'>
    <view id="shareboxs" catchtap='openreport'>
      <view id="shareboxsclose" style='position:relative;padding:23px 0 21px 0;'>
        <view id="reporttext">请选择举报原因</view>
      <image src="../img/addprice/chujia-btn-close.png" catchtap='closereport' style='position:absolute;top:10px;right:10px;'></image>
      </view>
      <view id="shareboxcont1">
