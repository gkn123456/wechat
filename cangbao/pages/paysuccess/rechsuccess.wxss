/* pages/paysuccess/rechsuccess.wxss */
@import "../template/template.wxss";
page{
  width:100%;
  height: 100%;
  background: #fff;
}
.header{
  width:100%;
  height:342rpx;
  background:rgba(255,255,255,1);
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top:248rpx;
  box-sizing: border-box;
}
.header1{
  display: flex;
  align-items: center;
}
.header1 image{
  width:40rpx;
  height:40rpx;
}
.header1 text{
  font-size:40rpx;
  font-weight:bold;
  color:rgba(66,66,66,1);
  margin-left:20rpx;
}
.header2{
   display: flex;
    align-items: center;
    width:100%;
    margin-top:232rpx;
    padding:0 40rpx;
    box-sizing: border-box;
}
.header2left{
  width:100%;
  height:88rpx;
  background:rgba(255,55,64,1);
  border-radius:4px;
  font-size:16px;
  font-weight:bold;
  color:rgba(255,255,255,1);
  line-height:88rpx;
  text-align: center;
}
.header2right{
  width:176rpx;
  height:60rpx;
  border-radius:42rpx;
  border:2rpx solid rgba(236,82,23,1);
  font-size:24rpx;
  font-weight:400;
  color:rgba(236,82,23,1);
  line-height: 60rpx;
  margin-left: 40rpx;
  text-align: center;
}
.like{
  width:100%;
  height:790rpx;
  background:rgba(255,255,255,1);
  margin-top:20rpx;
}