/* pages/videos/auction.wxss */
@import "../template/template.wxss";
@import "../../app.wxss";
page{
  background: #fff;
}
.headbox{
  width: 100%;
  background:linear-gradient(180deg,rgba(255,255,255,1) 0%,rgba(234,234,234,1) 100%);
  display: flex;
  padding:32rpx;
  box-sizing: border-box;
  position: relative;
}
.g_t_1{
  font-size:13px;
  line-height: 13px;
  color:rgba(68,68,68,1);
}
.iscolor{
  width:28px;
  height:28px;
}
.g_t_2{
  font-size:16px;
  line-height: 16px;
  color:#444;
}
.headimgs{
  width:218rpx;
  height:300rpx;
  border-radius:2rpx;
  background: #ccc;
}
.headimg{
  width:100%;
  height:100%;
}
.headcont{
  margin-left:32rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 63%;
}
.h_c_1{
  font-size:40rpx;
  font-weight:500;
  color:rgba(59,59,59,1);
  width:84%;
}
.h_c_2{
  font-size:28rpx;
  line-height: 28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  display: flex;
}
.h_c_3{
  font-size:22rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  display: flex;
  align-items: center;
}
.h_c_5{
  font-size:22rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  display: flex;
  align-items: center;
}
.h_c_6{
  font-size:22rpx;
  line-height: 22rpx;
  color:rgba(68,68,68,1);
  display: flex;
}

.h_c_4{
  font-size:22rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  display: flex;
  align-items: center;
}
.h_c_4 .text{
  padding:0 6rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  line-height: 32rpx;
  box-sizing: border-box;
  border-radius:2rpx;
  border:2rpx solid rgba(247,124,115,1);
  font-size:22rpx;
  font-weight:400;
  color:rgba(244,67,54,1);
}
.img1{
