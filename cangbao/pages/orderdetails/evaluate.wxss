/* pages/orderdetails/evaluate.wxss */
@import "../template/template.wxss";
page{
  background: #fff;
}
.head{
  margin-top:20rpx;
}
.top{
  display: flex;
  align-items: center;
  background: #fff;
  padding:22rpx 32rpx;
  box-sizing: border-box;
}
.top image{
  width:48rpx;
  height:48rpx;
  border-radius: 50%;
}
.top text{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  margin-left:24rpx;
}
.bottom{
  display: flex;
  align-items: center;
  background: #fff;
  padding:32rpx 32rpx;
  box-sizing: border-box;
  margin-top:2rpx;
}
.left{
  width:168rpx;
  height:168rpx;
}
.right{
  display: flex;
  flex-direction: column;
  margin-left: 32rpx;
}

.rtext1{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.rtext2{
  font-size:20rpx;
  font-weight:400;
  color:rgba(208,208,208,1);
}
.rtext3{
  margin-top:38rpx;
}
.rtext3_1{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.rtext3_2{
  font-size:32rpx;
  font-weight:400;
  color:rgba(28,28,28,1);
}
.evalcont{
  width: 100%;
  background: #fff;
  margin-top:20rpx;
  padding-top:34rpx;
  box-sizing: border-box;
}
.evalconthead{
  display: flex;
  align-items: center;
}
.evalconthead image{
  width:48rpx;
  height:48rpx;
  margin-left:32rpx;
}
.evalconthead text{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
  margin-left:20rpx;
}

.comment1-description{
  padding:30rpx 0;
  display:flex;
  flex-direction:row;
}
.stars1{
  display: flex;
  justify-content: center;
  padding:34rpx 0;
}
.stars{
    background:url("data:image/png;base64,");
    width: 46rpx;
    height: 44rpx;
    background-size: 46rpx 44rpx;
    margin-left: 40rpx;
    transition: 600ms;
}
.on{
    background:url("data:image/png;base64,");
    width: 46rpx;
    height: 44rpx;
    background-size: 46rpx 44rpx;
    transition: 1s;
    transform: scale(1,1)
}
.weui-cells_after-title{
  width: 100%;
  margin:0 auto;
  padding:40rpx 32rpx 16rpx 32rpx;
  box-sizing: border-box;
}

.btnSf{
  width: 92%;
  margin:30rpx auto;
  
}
.btnSf button{
    background: #57B6BA;
    color: #fff;
}

.btnSf button:active{
  background: #5BC2C6;
  color: #fff;
}
.weui-textarea-counter{
  text-align:right;
  font-size:24rpx;
  font-weight:400;
  color:rgba(208,208,208,1);
  position: absolute;
  bottom:20rpx;
  right:32rpx;
}
textarea{

}
.weui-textarea{
  width:100%;
  background:rgba(246,246,246,1);
  border-radius:12rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(95,95,95,1);
  padding:16rpx 32rpx 52rpx 32rpx;
}
.weui-cell__bd{
   width:100%;
  display: flex;
  justify-content: center;
  position: relative;
}
.weui-cell{
  display: flex;
  justify-content: center;
}
.tip{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding:0 32rpx;
  box-sizing: border-box;
}
.tipz{
  display: flex;
  align-items: center;

}
.tipz image{
  width:38rpx;
  height:38rpx;
  margin-right:26rpx;
}
.tipz text{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.tipy{
  font-size:28rpx;
  font-weight:400;
  color:rgba(213,213,213,1);
}
.label1{
  display: flex;
  justify-content: center;
  padding:0 50rpx;
  box-sizing: border-box;
  flex-wrap: wrap;
}
.label1 view{
  margin-right:20rpx;
  margin-top:20rpx;
}
.label1 view:nth-child(3){
  margin-right:0;
}
.label1 view:nth-child(6){
  margin-right:0;
}

.lablist{
  width:190rpx;
  height:76rpx;
  border-radius:38rpx;
  border:2rpx solid rgba(222,222,222,1);
  font-size:28rpx;
  font-weight:400;
  color:rgba(95,95,95,1);
  line-height:76rpx;
  text-align: center;
}
.label1 .c{
  width:190rpx;
  height:76rpx;
  border-radius:38rpx;
  border:2rpx solid rgba(222,222,222,1);
  font-size:28rpx;
  font-weight:400;
  color:#fff;
  line-height:76rpx;
  text-align: center;
  background:rgba(236,82,23,1);
}
.bt{
  position: fixed;
  bottom:0;
  left: 0;
  width: 100%;
  background:rgba(244,67,54,1);
  padding:32rpx;
  box-sizing: border-box;
  font-size:36rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
}