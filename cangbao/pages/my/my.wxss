page{
  background: #F8F8F8;
}
/* 自定义导航高度 */
.nav{
  width: 100%;
  height: 110rpx;
  overflow: hidden;
  z-index: 10;
  position: relative;
}
#myedit{
  width:18px;
  height: 17px;
  margin-left: 8px;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
  letter-spacing:2px;
}
.nav .back{
  width: 22px;
  height: 22px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 10px 15px;
}
.bg-white{
  background: #393939;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}
.fill{
  width: 100%;
  height: 50rpx;
}
/* 填充 */
.headpadding{
  width:100%;
}
/* 头部样式 */
.myhead{
  width:100%;
  background: #393939;
  position: relative;
}
.headcontbox{
  width:100%;
  height:360rpx;
  position: relative;
}
/* 未登录 */
.notloginbox{
  width:100%;
  height: 128rpx;
  display: flex;
  align-items: center;
  padding-left: 32rpx;
  box-sizing: border-box;
}
.logintext{
  color: #DDDDDD;
  margin-left: 40rpx;
  background: rgba(0,0,0,0);
}
.service{
  background: rgba(0,0,0,0);
  width:100%;
  height: 100%;
  padding:0;
  margin:0;
  display: flex;
  align-items: center;
  flex-direction: column;
}
/* 消除button边框 */
.logintext::after {
  border: none;
}
.service::after{
  border: none;
}
/* 用户头像 */
.user_img{
  width:128rpx;
  height:128rpx;
  border-radius: 50%;
}
/* 买家-卖家切换按钮 */
.terminalchange{
  width:150rpx;
  height: 52rpx;
  border: 1px solid #DEDEDE;
  border-radius: 14px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.terminalchange text{
  font-size:24rpx;
  color: #FFFFFF;
  margin-right: 16rpx;
}
.terminalimg{
  width:28rpx;
  height: 28rpx;
}
/* 买家端 */
.buyersidebox{
  width:100%;
  height: 128rpx;
  display: flex;
  justify-content: space-between;
  padding:0 36rpx 0 32rpx;
  box-sizing: border-box;
}
.buyerside_left{
  display: flex;
  width:76%;
}
.buyerside_usericon{
  width:128rpx;
  height:128rpx;
  position: relative;
}
.user_authentication_img{
  width:40rpx;
  height:40rpx;
  position: absolute;
  bottom:0;
  right: 0;
}
.user_namebox{
  height:100%;
  margin-left: 42rpx;
  width:70%;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.user_name{
  font-size:22px;
  width:98%;
  line-height: 24px;
  color: #FFFFFF;
  font-weight: bold;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  display: flex;
  align-items: flex-end;
}
#usernames{
  font-size:22px;
  position: fixed;
  top:-45px;
  left: 0;
  width:auto;
  display: flex;
}
.gouser{
  display: flex;
  align-items: center;
  margin-top:20rpx;
}
.gouser text{
  font-size:28rpx;
  color: #DDDDDD;
}
.gouser image{
  width:36rpx;
  height: 36rpx;
}
/* 买家功能 */
