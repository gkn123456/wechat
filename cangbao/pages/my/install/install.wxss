  padding-right:48rpx;
  box-sizing: border-box;
}
.header3{
  height:90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:48rpx;
  box-sizing: border-box;
}
.header3 text{
  font-size:28rpx;
  font-weight:400;
  color:rgba(28,28,28,1);
}
.header3to{
  width: 16rpx;
  height: 28rpx;
}
.b_n{
  display: flex;
