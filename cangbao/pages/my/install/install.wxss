@import "../../template/template.wxss";
.setheader{
  width:100%;
  background:rgba(255,255,255,1);
  margin-top: 20rpx;
  padding-left:32rpx;
  box-sizing: border-box;
}
.header1{
  height:154rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:48rpx;
  box-sizing: border-box;
}
.header1z{
  display: flex;
  align-items: center;
}
.header1z text{
  font-size:32rpx;
  font-weight:500;
  color:rgba(28,28,28,1);
  margin-left: 20rpx;
}
.header1zimg1{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.header1zimg2{
  width: 100rpx;
  height: 32rpx;
  margin-left: 8rpx;
}
.header1y{
  width: 16rpx;
