/* pages/my/install/about.wxss */
@import "../../template/template.wxss";
.head{
  width: 100%;
  height: 492rpx;
  background: #fff;
  display: flex;
  justify-content: center;
}
.logo{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top:106rpx;
}
.logo_img{
  width: 176rpx;
  height: 176rpx;
}
.version{
  font-size:28rpx;
  font-weight:400;
  color:rgba(111,111,111,1);
  margin-top:16rpx;
}

.main1{
  width: 100%;
  background: #fff;
  padding:0 32rpx;
  box-sizing: border-box;
  margin-top:20rpx;
}
.lis{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding:22rpx 0;
  box-sizing: border-box;
}
.lis:first-child{
  border-bottom:1rpx solid #ECECEC;
}
.lis_text{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.lisimg{
  width:12rpx;
  height:22rpx;
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.lisimgs official-account{
  font-size:28rpx;
  font-weight:400;
  color:rgba(68,68,68,1);
}
.br{
  position: absolute;
  bottom:11px;
  left: 0;
  width:100%;
  text-align: center;
  font-size:10px;
  color:rgba(111,111,111,1);
}