@import "../../template/template.wxss";
.kb{
  width:100%;height:88rpx;
  position: fixed;
  top:0;
  left: 0;background: #fff;
  padding:0 32rpx;box-sizing: border-box;
  
}
.kbs{
  width:100%;height:100%;display: flex;align-items: center;
  box-shadow:0px 0px 4px 0px rgba(216,216,216,0.8);border-radius:3px;
}
.kb_libox{
  width:50%;height: 100%;
  display: flex;flex-direction: column;align-items:center;justify-content: center;
}
.kb_li{
  font-size:32rpx;font-weight:500;color:rgba(51,51,51,1);
}
.kb_lion{
  color:rgba(102,102,102,1);font-weight:400;
}
.kb_lis{
  width:15px;
  height:2px;
  background:rgba(244,67,54,1);
  border-radius:1px;
  margin-top:10rpx;
}
.kb_lions{
  background:rgba(244,67,54,0);
}
.head{
  padding-top:140rpx;
  box-sizing: border-box;
  background: #fff;
  width:100%;
  height:338rpx;
}
.heads{
  padding-top:140rpx;
  box-sizing: border-box;
  background: #fff;
  width:100%;
}
.headtitle{
  font-size:24rpx;
  font-weight:400;
  color:rgba(28,28,28,1);
  text-align: center;
}
.headtitle text{
  font-size: 28rpx;
}
.headprice{
  text-align: center;
  margin-top:20rpx;
}
.htext1{
  font-size:48rpx;
  font-weight:500;
  color:rgba(28,28,28,1);
}
.htext2{
  font-size:64rpx;
  font-weight:500;
  color:rgba(28,28,28,1);
}
.htext3{
  font-size:28rpx;font-weight:400;color:rgba(51,51,51,1);margin-right: 20rpx;
}
.cont{
  background: #fff;
  padding-left:32rpx;
  box-sizing: border-box;
  margin-top:20rpx;
}
.licont{
  background: #fff;
  padding:28rpx 0 28rpx 0;
  box-sizing: border-box;
  border-top:1rpx solid #ECECEC;
}
.licont:nth-child(1){
  border-top:0rpx solid #ECECEC;
}
.litop{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:32rpx;
  box-sizing: border-box;
}
.libottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:32rpx;
  box-sizing: border-box;
}
.toptext1{
  font-size:28rpx;
  font-weight:400;
  color:rgba(28,28,28,1);
}
.toptext2{
  font-size:24rpx;
  font-weight:400;
  color:rgba(170,170,170,1);
}
.bottomtext1{
  font-size:24rpx;
  font-weight:400;
  color:rgba(170,170,170,1);
}
.bottomtext2{
  font-size:36rpx;
  font-weight:500;
  color:rgba(60,149,102,1);
}
.bottomtext3{
  font-size:36rpx;
  font-weight:500;
  color:rgba(28,28,28,1);
}
.but_box{
  width:100%;
  display: flex;justify-content: center;align-items: center;margin-top:48rpx;
  padding-bottom:60rpx;
}
.but_box1{
width:232rpx;
height:60rpx;
border-radius:3px;
border:1px solid rgba(185,185,185,1);
font-size:28rpx;
line-height: 60rpx;
font-weight:500;
text-align: center;
color:rgba(68,68,68,1);
}
.but_box2{
background:rgba(244,67,54,1);
color:rgba(255,255,255,1);
width:232rpx;
height:60rpx;
border-radius:3px;
font-size:28rpx;
line-height: 60rpx;
font-weight:500;
text-align: center;
margin-left: 20rpx;
}
.cashframebox{
  position: fixed;top: 0;left: 0;width: 100%;height: 100%;background: rgba(0,0,0,0.4);z-index: 20;
  display: flex;justify-content: center;align-items: center;
}
.cashframe{
  width:100%;padding:0 76rpx;box-sizing: border-box;
}
.cashframes{
  width:100%;background: #fff;border-radius:3px;overflow: hidden;
}
.cashframesfooter{
  width:100%;height: 86rpx;border-top:1px solid #ececec;font-size:34rpx;color:rgba(244,67,54,1);
  line-height: 86rpx;text-align: center;
}
.cashframestitle{
  font-size:36rpx;color:rgba(17,17,17,1);width:100%;text-align: center;padding-top:52rpx;
}
.balance{
  width:100%;display: flex;justify-content: space-between;
}
.balance:first-child{
  margin-bottom:44rpx;
}
.balances{
  width:100%;display: flex;align-items: center;
}
.balcontent{
  width:136rpx;
  height:40rpx;
  background:rgba(255,245,245,1);
  border-radius:2px;
  border:1px solid rgba(248,93,100,1);
  font-size:22rpx;
  font-weight:500;
  color:rgba(255,55,64,1);
  text-align: center;
  line-height: 40rpx;
}
.balcontenton{

}
.baltext1{
  font-size:32rpx;font-weight:500;color:rgba(51,51,51,1);margin-left:24rpx;
}
.radio{
  width:40rpx;height:40rpx;display: flex;
}
.radio-group{
  padding:50rpx 40rpx 40rpx 40rpx;box-sizing: border-box;
}