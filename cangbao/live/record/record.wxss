@import "../template/template.wxss";
.nav{
  box-shadow:0px 0px 4px 0px rgba(79,79,79,0.21);
}
page{
  width:100%;
  height: 100%;
  background: #fff;
}
.page{
  padding:0 16px;
  box-sizing: border-box;
  padding-bottom:50px;
}
.li{
  width:100%;
  border-bottom:1px solid #ECECEC;
  padding:12px 0 8px 0;
}
.li_title{
  width:100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.li_title1{
  font-size:14px;
  color:rgba(51,51,51,1);
  line-height:20px;
}
.li_title2{
  font-size:14px;
  color:rgba(51,51,51,1);
  line-height:20px;
}
.li_c1{
  font-size:12px;
  color:rgba(136,136,136,1);
  line-height:17px;
  margin-top:4px;
}
.line{
  width:100%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top:20px;
}
.lines{
  width:60px;
  height:1px;
  background: #ECECEC;
}
.line_font{
  font-size:8px;
  color:rgba(170,170,170,1);
  line-height:11px;
}