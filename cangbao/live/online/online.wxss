@import "../template/template.wxss";
.head{
  width:100%;
  height:98px;
  background:rgba(255,255,255,1);
  padding:14px 16px;box-sizing: border-box;
}
.head_title{
  font-size:16px;
  color:rgba(68,68,68,1);
  line-height:16px;
}
.head_count{
  width:100%;
  display: flex;
  align-items: center;
  margin-top:14px;
}
.head_count image{
  width:40px;height:40px;
  border-radius: 50%;
}
.head_count view{
  font-size:14px;
  color:rgba(51,51,51,1);
  line-height:20px;
  margin-left: 10px;
}
.room_line{
  width:100%;margin-top:10px;
  padding:14px 16px;box-sizing: border-box;
  background: #fff;
}
.room_line_title{
  font-size:16px;
  color:rgba(68,68,68,1);
  line-height:16px;
}
.li{
  width:100%;height: 60px;
  display: flex;align-items: center;
  border-bottom:1px solid #ECECEC;
  box-sizing: border-box;
}
.li:last-child{
  border:none;
}
.li image{
  width:40px;height:40px;
  border-radius: 50%;
}
.li view{
  font-size:14px;
  color:rgba(51,51,51,1);
  line-height:20px;
  margin-left: 10px;
}
