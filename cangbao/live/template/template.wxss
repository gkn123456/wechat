/* pages/template/template.wxss */
/* 自定义导航高度 */
.nav{
  width: 100%;
  height: 110rpx;
  overflow: hidden;
  z-index: 10;
  position: fixed;
  top:0;
  left:0;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
  letter-spacing:2px;
}
.o{
  color: #fff;
}
.nav-title_icon{
  position: absolute;
  height: 100%;
  top:0;
  left: 0;
  display: flex;
  align-items: center;
}
.nav .back{
  width: 9px;
  height: 14px;
  position: absolute;
  top:50%;
  left:19px;
  margin-top:-7px;
}
.backhome{
  width: 14px;
  height: 16px;
  position: absolute;
  top:50%;
  right:15px;
  margin-top:-8px;

}
.backhome1{
  width: 14px;
  height: 16px;
  
}
.f_hback{
  width: 87px;
  height: 32px;
}
.fh_box{
  width: 87px;
  height: 32px;
  margin-left: 32rpx;
  position: relative;
}
.fh_box1{
  width: 46px;
  height: 32px;
  margin-left: 32rpx;
  border: 1rpx solid rgba(0,0,0,0.08);
  border-radius: 16px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #f7f7f7;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}